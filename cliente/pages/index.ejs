<!DOCTYPE html>
<html lang="pt-BR">

<head>
    <!-- Title -->
    <title>
        <%- title %>
    </title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <meta name="author" content="<%- author %>" />
    <!-- description -->
    <meta name="description" content="<%- descricao %>" />
    <!-- keywords -->
    <meta name="keywords" content="<%- keywords %>" />
    <!-- animation -->
    <link rel="stylesheet" href="/css/plugins/animate.css" />
    <!-- Animated Headlines -->
    <link rel="stylesheet" href="/css/plugins/animated-headlines.css">
    <!-- bootstrap -->
    <link rel="stylesheet" href="/css/bootstrap.min.css" />
    <!-- font-awesome icon -->
    <link href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">
    <!-- Google Font -->
    <link href="https://fonts.googleapis.com/css?family=Muli:200,300,400,600,700,800,900" rel="stylesheet">
    <!-- Animated Headlines -->
    <link rel="stylesheet" href="/css/plugins/animated-headlines.css">
    <!-- magnific popup -->
    <link rel="stylesheet" href="/css/plugins/magnific-popup.css" />
    <!-- Main Style -->
    <link rel="stylesheet" href="/css/main.css" />

    <meta property="og:title" content="<%- title %>">
    <meta property="og:site_name" content="<%- title %>">
    <meta property="og:url" content="http://www.rodrigomsdev.io">
    <meta property="og:description" content="<%- descricao %>">
    <meta property="og:type" content="profile">
    <meta property="og:image" content="<%- imgOG %>">

</head>

<body>
    <!-- ===== Start Navbar ===== -->
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark fixed-top">
        <div class="container">
            <a class="navbar-brand" href="#"><img src="/img/logo.png" alt=""></a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarNav">
                <ul class="navbar-nav ml-auto">
                    <li class="nav-item">
                        <a class="nav-link" href="#slider">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#sobremim">Sobre</a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    <!-- ===== End Navbar ===== -->

    <!-- ===== Start Slider ===== -->
    <section class="slider d-flex justify-content-center" id="slider">
        <div class="container text-center">
            <div class="row all-content">
                <div class="col-12">
                    <div class="left-content">
                        <p class="hello">
                            Olá, eu sou</p>
                        <!-- Animated Text-->
                        <div class="animated-headlines">
                            <h1 class="cd-headline clip is-full-width">
                                <span class="cd-words-wrapper">
                                    <b class="is-visible"><%- nome %></b>
                                    <b><%- cargo %></b>
                                </span>
                            </h1>
                        </div>
                        <p class="paragraph">
                            <%- descricaoParagrafo %>
                        </p>
                        <a href="#0" class="btn">
                            Baixar CV</a>
                    </div>
                </div>
            </div>
        </div>
        <div id="particles-js" class="particels"></div>
    </section>
    <!-- ===== End Slider ===== -->



    <!-- ===== Start About Me ===== -->
    <section class="about-me" id="sobremim">
        <div class="container">
            <div class="row">
                <div class="col-md-5">
                    <img src="https://via.placeholder.com/400x350" alt="" class="/img-fluid">
                </div>
                <div class="col-md-7">
                    <h2>Sobre mim</h2>
                    <p>
                        <%- sobreMim %>
                    </p>
                    <a href="#0" class="btn-1">Download CV</a>
                    <a href="#0" class="btn-2">Portfolio</a>
                </div>
            </div>
        </div>
    </section>
    <!-- ===== End About Me ===== -->

    <!-- ===== Start Services ===== -->
    <section class="services" id="services">
        <h2 class="text-center">Conhecimentos</h2>
        <div class="container">
            <div class="row">
                <!-- Box-1 -->
                <div class="col-md-6 col-lg-4">
                    <div class="box">
                        <span class="fa fa-cog"></span>
                        <h3>Design Criativo</h3>
                    </div>
                </div>
                <!-- Box-2 -->
                <div class="col-md-6 col-lg-4">
                    <div class="box">
                        <span class="fa fa-crop"></span>
                        <h3>Código Limpo</h3>
                    </div>
                </div>
                <!-- Box-3 -->
                <div class="col-md-6 col-lg-4">
                    <div class="box">
                        <span class="fa fa-cubes"></span>
                        <h3>Design Responsivo</h3>
                    </div>
                </div>
                <!-- Box-4 -->
                <div class="col-md-6 col-lg-4">
                    <div class="box">
                        <span class="fa fa-code"></span>
                        <h3>JavaScript</h3>
                    </div>
                </div>
                <!-- Box-5 -->
                <div class="col-md-6 col-lg-4">
                    <div class="box">
                        <span class="fa fa-code"></span>
                        <h3>Node.JS</h3>
                    </div>
                </div>
                <!-- Box-6 -->
                <div class="col-md-6 col-lg-4">
                    <div class="box">
                        <span class="fa fa-code"></span>
                        <h3>PHP</h3>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- ===== End Services ===== -->

    <!-- ===== Start Some Facts ===== -->
    <section class="some-facts">
        <div class="container text-center">
            <div class="row">
                <!-- Box-1 -->
                <div class="col-lg-4 col-sm-6">
                    <div class="items">
                        <span class="fa fa-skyatlas"></span>
                        <h3>
                            <span class="counter"><%- clientes_felizes %></span>
                        </h3>
                        <div class="line mx-auto"></div>
                        <h4>Clientes Felizes</h4>
                    </div>
                </div>
                <!-- Box-2 -->
                <div class="col-lg-4 col-sm-6">
                    <div class="items">
                        <span class="fa fa-heart-o"></span>
                        <h3>
                            <span class="counter"><%- projetos_completos %></span>
                        </h3>
                        <div class="line mx-auto"></div>
                        <h4>Projetos completos</h4>
                    </div>
                </div>
                <!-- Box-3 -->
                <div class="col-lg-4 col-sm-6">
                    <div class="items">
                        <span class="fa fa-globe"></span>
                        <h3>
                            <span class="counter"><%- projetos_baixados %></span>
                        </h3>
                        <div class="line mx-auto"></div>
                        <h4>Arquivos Baixados</h4>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- ===== End Some Facts ===== -->

    <!-- ===== Start Contact ===== -->
    <section class="contact" id="contact">
        <h2 class="text-center">Mantenha contato</h2>
        <div class="container">
            <div class="row">
                <div class="col-md-2"></div>
                <div class="col-md-8">
                    <form id="formContato">
                        <div class="row">
                            <div class="col-sm-6">
                                <input type="text" class="form-control" id="nome_contato" placeholder="Nome">
                            </div>
                            <div class="col-sm-6">
                                <input type="email" class="form-control" id="email_contato" placeholder="Email">
                            </div>
                            <div class="col-sm-12">
                                <input type="text" class="form-control" id="assunto_contato" placeholder="Assunto">
                            </div>
                        </div>
                        <div class="form-group">
                            <textarea class="form-control" rows="5" id="mensagem_contato" placeholder="Mensagem"></textarea>
                        </div>
                        <button class="btn btn-block" type="submit">Enviar</button>
                    </form>
                </div>
            </div>
        </div>
    </section>
    <!-- ===== End Contact ===== -->


    <!-- ===== Start Footer ===== -->
    <footer class="footer text-center">
        <div class="social-icons">
            <span class="fa fa-facebook" aria-hidden="true"></span>
            <span class="fa fa-linkedin" aria-hidden="true"></span>
        </div>
        <p>Rodrigo DEV S2 </p>
    </footer>
    <!-- ===== End Footer ===== -->

    <!-- ==== javascript libraries ==== -->
    <!-- jQuery-->
    <script src="/js/plugins/jquery-3.3.1.min.js"></script>
    <!-- Bootstrap 4.1.3 -->
    <script src="/js/bootstrap.min.js"></script>
    <script src="/js/plugins/popper.min.js"></script>
    <!-- Animated Headlines -->
    <script src="/js/plugins/animated-headlines.js"></script>
    <!-- Nav jQuery-->
    <script src="/js/plugins/nav.jquery.js"></script>
    <!-- Nav jQuery-->
    <script src="/js/plugins/jquery.stellar.js"></script>
    <!-- Isotope -->
    <script src="/js/plugins/isotope.pkgd.min.js"></script>
    <!-- Image Loaded -->
    <script src="/js/plugins/imagesloaded.pkgd.min.js"></script>
    <!-- Magnific Popup -->
    <script src="/js/plugins/magnific-popup.min.js"></script>
    <!-- Smooth-Scroll -->
    <script src="/js/smooth-scroll.js"></script>
    <!-- Counter Up -->
    <script src="/js/plugins/jquery.waypoints.min.js"></script>
    <script src="/js/plugins/jquery.counterup.min.js"></script>
    <!-- Particles Js -->
    <script src="/js/plugins/particles.min.js"></script>
    <script src="/js/plugins/app.js"></script>
    
    <script src="/js/main.js"></script>

</body>

</html>