<%- include('./header.ejs') %>
    <!-- ===== Start Slider ===== -->
    <section class="slider d-flex justify-content-center" id="slider">
        <div class="container text-center">
            <div class="row all-content">
                <div class="col-12">
                    <div class="left-content">
                        <p class="hello">
                            Olá, eu sou</p>
                        <!-- Animated Text-->
                        <div class="animated-headlines">
                            <h1 class="cd-headline clip is-full-width">
                                <span class="cd-words-wrapper">
                                    <b class="is-visible">
                                        <%- nome %>
                                    </b>
                                    <b>
                                        <%- cargo %>
                                    </b>
                                </span>
                            </h1>
                        </div>
                        <p class="paragraph">
                            <%- descricaoParagrafo %>
                        </p>
                        <a href="#0" class="btn">
                            Baixar CV</a>
                    </div>
                </div>
            </div>
        </div>
        <div id="particles-js" class="particels"></div>
    </section>
    <!-- ===== End Slider ===== -->



    <!-- ===== Start About Me ===== -->
    <section class="about-me" id="sobremim">
        <div class="container">
            <div class="row">
                <div class="col-md-5">
                    <img src="https://via.placeholder.com/400x350" alt="" class="/img-fluid">
                </div>
                <div class="col-md-7">
                    <h2>Sobre mim</h2>
                    <p>
                        <%- sobreMim %>
                    </p>
                    <a href="#0" class="btn-1">Download CV</a>
                    <a href="#0" class="btn-2">Portfolio</a>
                </div>
            </div>
        </div>
    </section>
    <!-- ===== End About Me ===== -->

    <!-- ===== Start Services ===== -->
    <section class="services" id="Conhecimentos">
        <h2 class="text-center">Conhecimentos</h2>
        <div class="container">
            <div class="row">
                <!-- Box-1 -->
                <div class="col-md-6 col-lg-4">
                    <div class="box">
                        <span class="fa fa-cog"></span>
                        <h3>Design Criativo</h3>
                    </div>
                </div>
                <!-- Box-2 -->
                <div class="col-md-6 col-lg-4">
                    <div class="box">
                        <span class="fa fa-crop"></span>
                        <h3>Código Limpo</h3>
                    </div>
                </div>
                <!-- Box-3 -->
                <div class="col-md-6 col-lg-4">
                    <div class="box">
                        <span class="fa fa-cubes"></span>
                        <h3>Design Responsivo</h3>
                    </div>
                </div>
                <!-- Box-4 -->
                <div class="col-md-6 col-lg-4">
                    <div class="box">
                        <span class="fa fa-code"></span>
                        <h3>JavaScript</h3>
                    </div>
                </div>
                <!-- Box-5 -->
                <div class="col-md-6 col-lg-4">
                    <div class="box">
                        <span class="fa fa-code"></span>
                        <h3>Node.JS</h3>
                    </div>
                </div>
                <!-- Box-6 -->
                <div class="col-md-6 col-lg-4">
                    <div class="box">
                        <span class="fa fa-code"></span>
                        <h3>PHP</h3>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- ===== End Services ===== -->

    <!-- ===== Start Some Facts ===== -->
    <section class="some-facts">
        <div class="container text-center">
            <div class="row">
                <!-- Box-1 -->
                <div class="col-lg-4 col-sm-6">
                    <div class="items">
                        <span class="fa fa-skyatlas"></span>
                        <h3>
                            <span class="counter">
                                <%- clientes_felizes %>
                            </span>
                        </h3>
                        <div class="line mx-auto"></div>
                        <h4>Clientes Felizes</h4>
                    </div>
                </div>
                <!-- Box-2 -->
                <div class="col-lg-4 col-sm-6">
                    <div class="items">
                        <span class="fa fa-heart-o"></span>
                        <h3>
                            <span class="counter">
                                <%- projetos_completos %>
                            </span>
                        </h3>
                        <div class="line mx-auto"></div>
                        <h4>Projetos completos</h4>
                    </div>
                </div>
                <!-- Box-3 -->
                <div class="col-lg-4 col-sm-6">
                    <div class="items">
                        <span class="fa fa-globe"></span>
                        <h3>
                            <span class="counter">
                                <%- projetos_baixados %>
                            </span>
                        </h3>
                        <div class="line mx-auto"></div>
                        <h4>Arquivos Baixados</h4>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- ===== End Some Facts ===== -->


    <!-- ===== Start Blog ===== -->
    <section class="blog" id="blog">
        <div class="container">
            <h2 class="text-center">Baixar Projetos</h2>

            <span id="projetosDownloadBody"></span>

            <% dadosFor.forEach(function(item) { %>
            <div class="row">
                <div class="co-md-6 col-lg-4">
                    <div class="box">
                        <div class="image">
                            <img src="<%= item.img %>" style="height: max-250px; width: max-400px;" alt="">
                            <a href="" class="cate"><%= item.linguagem %></a>
                        </div>
                        <div class="text">
                            <h3><a href="<%= item.href %>"><%= item.linkDownload %></a></h3>
                            <h4>By Rodrigo</h4>
                            <div class="col-md-12"><button class="btn btn-block" data-id="<%= item.id %>" data-url="<%= item.url_api %>" data-nome="<%= item.linkDownload %>" id="downloadProjeto">Baixar</button></div>
                        </div>
                    </div>
                </div>
            </div>
            <% }); %>
        </div>
    </section>
    <!-- ===== End Blog ===== -->


    <!-- ===== Start Contact ===== -->
    <section class="contact" id="contact">
        <h2 class="text-center">Mantenha contato</h2>
        <div class="container">
            <div class="row">
                <div class="col-md-2"></div>
                <div class="col-md-8">
                    <form id="formContato">
                        <div class="row">
                            <div class="col-sm-6">
                                <input type="text" class="form-control" id="nome_contato" placeholder="Nome">
                            </div>
                            <div class="col-sm-6">
                                <input type="email" class="form-control" id="email_contato" placeholder="Email">
                            </div>
                            <div class="col-sm-12">
                                <input type="text" class="form-control" id="assunto_contato" placeholder="Assunto">
                            </div>
                        </div>
                        <div class="form-group">
                            <textarea class="form-control" rows="5" id="mensagem_contato" placeholder="Mensagem"></textarea>
                        </div>
                        <button class="btn btn-block" type="submit">Enviar</button>
                    </form>
                </div>
            </div>
        </div>
    </section>
    <!-- ===== End Contact ===== -->


    <%- include('./footer.ejs') %>